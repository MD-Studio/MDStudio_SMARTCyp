{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://mdstudio/schemas/endpoints/smartcyp_response.v1.json",
  "title": "MDStudio SMARTCyp prediction",
  "description": "Perform a CYP site of metabolism prediction using SMARTCyp",
  "type": "object",
  "properties": {
    "status": {
      "type": "string",
      "description": "Status of SMARTCyp info request",
      "enum": [
        "failed",
        "completed"
      ]
    },
    "result": {
      "type": "object",
      "description": "Content of SMARTCyp prediction .csv file"
    }
  },
  "required": [
    "status"
  ]
}