{
  "kind": "Template",
  "apiVersion": "v1",
  "metadata": {
    "name": "mdstudio_smartcyp",
    "namespace": "openrisknet",
    "annotations": {
      "openshift.io/display-name": "Cytochrome P450-Mediated Drug Metabolism",
      "openshift.io/provider-display-name": "Vrij Universiteit Amsterdam, AIMMS Division of Molecular and Computational Toxicology",
      "openshift.io/documentation-url": "https://github.com/MD-Studio/MDStudio_SMARTCyp",
      "openshift.io/support-url": "https://github.com/MD-Studio/MDStudio_SMARTCyp/issues",
      "iconClass": "icon-3scale",
      "description": "3D structure and 2D reactivity-based Prediction of Cytochrome P450-Mediated Drug Metabolism",
      "tags": "MDStudio, SMARTCyp, PLANTS, SPORES, docking, CYP, P450, SOM, prediction"
    },
    "labels": {
      "template": "mdstudio_smartcyp",
      "app": "mdstudio_smartcyp"
    }
  },
  "message": "MDStudio SMARTCyp has been deployed to your project. To remove the deployment you can use the command-line and run \"oc delete all --selector template=mdstudio_smartcyp\" for the project you deployed to.",
  "parameters": [
    {
      "name": "IMAGE_TAGDOCKER",
      "value": "latest"
    },
    {
      "name": "ROUTES_BASENAME",
      "value": "prod.openrisknet.org"
    }
  ],
  "objects": [
    {
      "kind": "DeploymentConfig",
      "apiVersion": "v1",
      "metadata": {
        "name": "mdstudio_smartcyp"
      },
      "spec": {
        "replicas": 1,
        "revisionHistoryLimit": 5,
        "selector": {
          "name": "mdstudio_smartcyp"
        },
        "template": {
          "metadata": {
            "labels": {
              "name": "mdstudio_smartcyp"
            }
          },
          "spec": {
            "containers": [
              {
                "name": "mdstudio_smartcyp",
                "image": "mdstudio/mdstudio_smartcyp:${IMAGE_TAGDOCKER}",
                "ports": [
                  {
                    "containerPort": 8081
                  }
                ],
                "readinessProbe": {
                  "failureTreshold": 6,
                  "httpGet": {
                    "path": "/ui/",
                    "port": 8081,
                    "scheme": "HTTP"
                  },
                  "initialDelaySeconds": 300,
                  "periodSeconds": 30,
                  "timeoutSeconds": 1
                },
                "livenessProbe": {
                  "failureTreshold": 6,
                  "httpGet": {
                    "path": "/ui/",
                    "port": 8081,
                    "scheme": "HTTP"
                  },
                  "initialDelaySeconds": 300,
                  "periodSeconds": 30,
                  "timeoutSeconds": 1
                }
              }
            ]
          }
        }
      }
    },
    {
      "kind": "Service",
      "apiVersion": "v1",
      "metadata": {
        "name": "mdstudio_smartcyp"
      },
      "spec": {
        "ports": [
          {
            "name": "mdstudio_smartcyp",
            "port": 8081,
            "targetPort": 8081
          }
        ],
        "selector": {
          "name": "mdstudio_smartcyp"
        }
      }
    },
    {
      "kind": "Service",
      "apiVersion": "v1",
      "metadata": {
        "name": "mdstudio_smartcyp-swagger"
      },
      "spec": {
        "ports": [
          {
            "name": "mdstudio_smartcyp-swagger",
            "port": 8081,
            "targetPort": 8081
          }
        ],
        "selector": {
          "name": "mdstudio_smartcyp"
        }
      }
    },
    {
      "kind": "Route",
      "apiVersion": "v1",
      "metadata": {
        "name": "mdstudio_smartcyp",
        "annotations": {
          "kubernetes.io/tls-acme": "false"
        }
      },
      "spec": {
        "host": "mdstudio_smartcyp.${ROUTES_BASENAME}",
        "to": {
          "kind": "Service",
          "name": "mdstudio_smartcyp"
        }
      }
    },
    {
      "kind": "Route",
      "apiVersion": "v1",
      "metadata": {
        "name": "mdstudio_smartcyp-swagger",
        "annotations": {
          "kubernetes.io/tls-acme": "false"
        }
      },
      "spec": {
        "host": "mdstudio_smartcyp.${ROUTES_BASENAME}",
        "path": "/ui/",
        "to": {
          "kind": "Service",
          "name": "mdstudio_smartcyp-swagger"
        }
      }
    }
  ]
}
